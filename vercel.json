{
    "redirects": [
        {
            "source": "/",
            "destination": "/console/",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/console",
            "destination": "/console/"
        },
        {
            "source": "/console/(.*)",
            "destination": "/$1"
        }
    ],
    "installCommand": "pnpm install --frozen-lockfile"
}