{
    "redirects": [
        {
            "source": "/",
            "destination": "/console/",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/console/(.*)",
            "destination": "/$1"
        },
        {
            "source": "/console/:path*",
            "destination": "/index.html"
        },
    ],
    "installCommand": "pnpm install --frozen-lockfile"
}