        "@popperjs/core": "^2.11.6",
        "@sentry/svelte": "^7.30.0",
        "@sentry/tracing": "^7.30.0",
        "analytics": "^0.8.1",
        "echarts": "^5.4.1",
        "pretty-bytes": "^6.0.0",
